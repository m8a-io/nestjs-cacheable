{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "globalSetup": "<rootDir>/globalSetup.ts",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@m8a-io/nestjs-cacheable": "<rootDir>/../libs/nestjs-cacheable/src",
    "@m8a-io/nestjs-cacheable/(.*)": "<rootDir>/../libs/nestjs-cacheable/src/$1"
  }
}